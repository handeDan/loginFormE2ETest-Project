  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0",
    "reactstrap": "^9.2.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react18": "^1.1.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.0",
    "cypress": "^13.17.0",
    "react-refresh": "^0.11.0",
    "start-server-and-test": "^2.0.9",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^3.0.1"
  },
  "browserslist": [
