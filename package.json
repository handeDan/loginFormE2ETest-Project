{
  "name": "vite-react-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
